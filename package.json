{
  "name": "@enginode/flow",
  "version": "0.0.1",
  "description": "A lowcode process engine written for node/browser.",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.cjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "unbuild",
    "format": "prettier -w ./{src,test}/**/*.ts",
    "test": "vitest"
  },
  "keywords": [
    "flow"
  ],
  "author": "Zhi <yfwz100@yeah.net>",
  "license": "ISC",
  "devDependencies": {
    "@types/lodash-es": "^4.17.6",
    "@types/node": "^18.11.18",
    "prettier": "^2.8.2",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.4",
    "unbuild": "^1.0.2",
    "vitest": "^0.26.3"
  },
  "dependencies": {
    "expression-eval": "^5.0.0",
    "typescript-json-decoder": "^1.0.11",
    "lodash-es": "^4.17.21"
  },
  "prettier": {
    "printWidth": 120,
    "quoteProps": "as-needed",
    "useTabs": false,
    "tabWidth": 2,
    "semi": true,
    "trailingComma": "all",
    "singleQuote": true
  }
}
